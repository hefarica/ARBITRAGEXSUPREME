{
  "name": "arbitragex-cloudflare-workers",
  "version": "3.0.0",
  "description": "ArbitrageX Supreme V3.0 - Cloudflare Workers Edge Computing Module",
  "main": "workers/api-worker.js",
  "scripts": {
    "dev": "wrangler dev",
    "dev:api": "wrangler dev workers/api-worker.js",
    "dev:engine": "wrangler dev workers/engine-worker.js --env engine",
    "deploy": "wrangler deploy",
    "deploy:api": "wrangler deploy workers/api-worker.js",
    "deploy:engine": "wrangler deploy workers/engine-worker.js --env engine",
    "tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create ARBITRAGEX_CACHE",
    "d1:create": "wrangler d1 create arbitragex-production",
    "d1:migrations": "wrangler d1 migrations apply arbitragex-production",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint workers/**/*.js",
    "format": "prettier --write workers/**/*.js"
  },
  "keywords": [
    "arbitrage",
    "mev",
    "cloudflare-workers",
    "edge-computing",
    "defi",
    "blockchain",
    "ethereum",
    "polygon",
    "binance-smart-chain",
    "avalanche",
    "flash-loans",
    "dex-aggregation"
  ],
  "author": "Hector Fabio Riascos C. - Ingenio Pichichi S.A.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hefarica/ARBITRAGEXSUPREME.git",
    "directory": "cloudflare-workers"
  },
  "dependencies": {
    "itty-router": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.78.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}